<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:gravity="center">
		<shape
			android:shape="line">
			<stroke
				android:width="3dp"
				android:color="#ffffff">
			</stroke>
		</shape>
	</item>
	<item
		android:gravity="center">
		<rotate
			android:fromDegrees="90"
			android:toDegrees="90">
		<shape
			android:shape="line">
			<stroke
				android:width="3dp"
				android:color="#ffffff">
			</stroke>
		</shape>
		</rotate>
	</item>
</layer-list>
